import os


board = []

def print_board():
    os.system('clear')
    for row in board:
        print(' '.join([str(x) for x in row ]))

def read_board():
    with open('sudoku.txt', 'r') as sudoku:
        for line in sudoku:
            board.append([int(a) for a in line.split()])

def possible_numbers_for_row(row):
    possible_numbers = [a for a in range(1,10)]
    for i in row:
        if i != 0:
            possible_numbers.remove(i)
    return possible_numbers

def possible_numbers_for_column(column):
    pass



def solve():
    for row in board:
        print(possible_numbers_for_row(row))




read_board()
print_board()
solve()
